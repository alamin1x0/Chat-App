<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.developeralamin.goponchart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/newchart"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/newchart"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoponChart">
        <activity android:name=".Specifchart"/>
        <activity android:name=".UpdateProfile" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ChartActivity" />
        <activity android:name=".Setprofile" />
        <activity android:name=".OtpAutheration" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SlappScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>